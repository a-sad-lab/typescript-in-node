{
  "compilerOptions": {
    "module": "commonjs",
    "importHelpers": true,
    "esModuleInterop": true,
    "target": "es2017",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noEmitOnError": true,
    "strict": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "composite": true
  }
}
